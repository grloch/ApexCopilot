{
	"name": "apex-copilot",
	"version": "0.0.0",
	"description": "pkg desc",
	"keywords": [
		"oclif"
	],
	"homepage": "https://github.com/grloch/ApexCopilot",
	"bugs": "https://github.com/grloch/ApexCopilot/issues",
	"repository": "grloch/ApexCopilot",
	"license": "MIT",
	"author": "Gabriel Loch",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"bin": {
		"apexCopilot": "./bin/run.js"
	},
	"files": [
		"/bin",
		"/dist",
		"/oclif.manifest.json"
	],
	"pnpm": {
		"overrides": {
			"lodash.pick@>=3.7.0 <4.17.19": "https://github.com/lodash/lodash/archive/f299b52f39486275a9e6483b60a410e06520c538.tar.gz"
		}
	},
	"scripts": {
		"build": "shx rm -rf dist && tsc -b",
		"lint": "eslint . --ext .ts",
		"prepack": "npm run build && oclif manifest && oclif readme",
		"postpack": "shx rm -f oclif.manifest.json",
		"prepare": "npm run build",
		"prettier": "prettier --write \"src/**/*\"",
		"test": "mocha --forbid-only \"test/**/*.test.ts\"",
		"posttest": "npm run lint",
		"version": "oclif readme && git add README.md"
	},
	"oclif": {
		"bin": "apexCopilot",
		"commands": "./dist/commands",
		"dirname": "apexCopilot",
		"plugins": [
			"@oclif/plugin-help",
			"@oclif/plugin-plugins"
		],
		"theme": ".oclif-theme.json",
		"topicSeparator": " ",
		"topics": {
			"global": {
				"description": "PKG JSON TOPIC DESC"
			},
			"package": {
				"description": "Package options"
			},
			"package:merge": {
				"description": "Merge two or more xml manifest files into one"
			},
			"project": {
				"description": "Configure options for local project"
			},
			"project:org": {
				"description": "Add, remove and list current project orgs"
			},
			"project:org:add": {
				"description": "PKG JSON TOPIC DESC"
			}
		}
	},
	"dependencies": {
		"@inquirer/checkbox": "^2.3.6",
		"@inquirer/input": "^2.1.1",
		"@inquirer/prompts": "^4.3.3",
		"@inquirer/select": "^2.3.5",
		"@oclif/core": "^3",
		"@oclif/plugin-help": "^6",
		"@oclif/plugin-plugins": "^5.3.2",
		"@salesforce/core": "^8.0.5",
		"chalk": "^5.3.0",
		"colors-cli": "^1.0.33",
		"glob": "^10.4.1",
		"inquirer": "^9.2.23",
		"inquirer-file-tree-selection-prompt": "^2.0.5",
		"minimatch": "^9.0.4",
		"table": "^6.8.2",
		"xml-js": "^1.6.11"
	},
	"devDependencies": {
		"@oclif/prettier-config": "^0.2.1",
		"@oclif/test": "^3",
		"@types/chai": "^4",
		"@types/fs-extra": "^11.0.4",
		"@types/inquirer": "^9.0.7",
		"@types/mocha": "^10.0.0",
		"@types/node": "^18",
		"@types/strip-color": "^0.1.2",
		"chai": "^4",
		"eslint": "^8.57.0",
		"eslint-config-oclif": "^5",
		"eslint-config-oclif-typescript": "^3",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-node": "^11.1.0",
		"fs-extra": "^11.2.0",
		"mocha": "^10",
		"oclif": "^4",
		"prettier": "^3.3.2",
		"shx": "^0.3.3",
		"strip-color": "^0.1.0",
		"ts-node": "^10.9.2",
		"typescript": "^5"
	},
	"engines": {
		"node": ">=18.0.0"
	}
}
