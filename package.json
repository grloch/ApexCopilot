{
  "name": "apex-copilot",
  "version": "0.0.0",
  "description": "pkg desc",
  "keywords": [
    "oclif"
  ],
  "homepage": "https://github.com/grloch/ApexCopilot",
  "bugs": "https://github.com/grloch/ApexCopilot/issues",
  "repository": "grloch/ApexCopilot",
  "license": "MIT",
  "author": "Gabriel Loch",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "apexCopilot": "./bin/run.js"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepare": "npm run build",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "posttest": "npm run lint",
    "version": "oclif readme && git add README.md"
  },
  "oclif": {
    "bin": "apexCopilot",
    "commands": "./dist/commands",
    "dirname": "apexCopilot",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "theme": ".oclif-theme.json",
    "topicSeparator": " ",
    "topics": {
      "global": {
        "description": "PKG JSON TOPIC DESC"
      },
      "project": {
        "description": "PKG JSON TOPIC DESC"
      }
    }
  },
  "dependencies": {
    "@inquirer/input": "^2.1.1",
    "@inquirer/prompts": "^4.3.1",
    "@oclif/core": "^3",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^4",
    "chalk": "^5.3.0",
    "colors-cli": "^1.0.33",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^3",
    "@types/chai": "^4",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18",
    "@types/strip-color": "^0.1.2",
    "chai": "^4",
    "eslint": "^8.57.0",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-node": "^11.1.0",
    "fs-extra": "^11.2.0",
    "mocha": "^10",
    "oclif": "^4",
    "shx": "^0.3.3",
    "strip-color": "^0.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
